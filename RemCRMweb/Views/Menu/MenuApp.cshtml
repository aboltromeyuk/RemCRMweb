@model RemCRMviewModel.Menu.MenuViewModel
@{
    Layout = null;
}

<div id="wrapper">
    <div id="sidebar-wrapper">
        <ul class="sidebar-nav">
            <li class="sidebar-brand">
                <a href="#">
                    @Model.Name
                </a>
            </li>
            @foreach (var item in Model.Items)
            {
                <li id="item">
                    <a id=@item.Url href=@item.Url onclick="return false">@item.Name</a>
                </li>
            }
        </ul>
    </div>
</div>
